{
  "name": "HandballCH-widget",
  "version": "0.1.0",
  "private": true,
  "license": "MPL-2.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^29.0.2",
    "@types/node": "^16.11.45",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.5.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-all": "concurrently \"yarn run start\" \"yarn run mock-api\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "pre-build": "replace-env public/_headers",
    "mock-api": "json-server --watch mock-api/db.json --routes mock-api/routes.json --port 3001"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "bulma": "^0.9.4",
    "concurrently": "^6.4.0",
    "http-proxy-middleware": "^2.0.1",
    "json-server": "^0.17.0",
    "replace-env": "^1.0.4",
    "sass": "^1.44.0"
  },
  "resolutions": {
    "@svgr/webpack": "^6.3.1"
  }
}
